{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "no-angle-bracket-type-assertion": false,
    "no-shadowed-variable": false,
    "variable-name": false,
    "no-implicit-dependencies": false,
    "ordered-imports": false,
    "no-submodule-imports": false
  },
  "linterOptions": {
    "exclude": [
      "src/dts-dom/*",
      "src/typings/*"
    ]
  }
}