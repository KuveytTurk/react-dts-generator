{
  "include": [
    "src",
    "baselines"
  ],
  "compilerOptions": {
    "lib": [
      "es6",
      "dom"
    ],
    "module": "commonjs",
		"target": "es6",
		"jsx": "react",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": false,
    "outDir": "./bin",
    "newLine": "LF",
    "esModuleInterop": true,
    "declaration": true
  }
}
